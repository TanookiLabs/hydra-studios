<div class="page-width mt-5 pt-5">
  {% if section.settings.title != blank %}
    <div class="text-center">
      <h2 class="logo-bar-title">{{ section.settings.title | escape }}</h2>
    </div>
    <div class="text-center">
      <p class="logo-bar-subtitle">{{ section.settings.subtitle | escape }}</p>
    </div>
  {% endif %}

  {% if section.blocks.size > 0 %}
    <div class="mt-5 d-flex align-items-center justify-content-around flex-wrap">
      {% for block in section.blocks %}
        <div class="text-center my-3">
          {% if block.settings.link != blank %}
            <a href="{{ block.settings.link }}" target="_blank">
          {% endif %}
          {% if block.settings.image != blank %}
            <img src={{ block.settings.image | image_url }} class="logo-bar-image" />
          {% endif %}
          {% if block.settings.link != blank %}
            </a>
          {% endif %}
        </div>
      {% endfor %}
    </div>

    <div class="text-center mb-5">
    </div>
  {% endif %}

  {% if section.blocks.size == 0 %}
    {% include 'no-blocks' %}
  {% endif %}
</div>



